    "@nteract/types": "^3.1.0",
    "rc-menu": "7.4.20",
    "react-redux": "^6.0.0",
    "redux": "^4.0.0",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc.12",
    "react": "^16.3.2"
  },
