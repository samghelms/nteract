{
  "name": "@nteract/presentational-components",
  "version": "2.0.0-alpha.0",
  "description": "pure presentational components for nteract",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "nteractDesktop": "src/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "tsc -b --clean",
    "build:lib": "tsc -b",
    "build:lib:watch": "tsc -b --watch",
    "build:watch": "npm run build:clean && npm run build:lib:watch"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.7.0",
    "@blueprintjs/select": "^3.2.0",
    "@nteract/styled-blueprintjsx": "^1.1.1",
    "react-syntax-highlighter": "^10.0.0",
    "styled-components": "^4.1.3",
    "styled-jsx": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^16.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nteract",
    "components"
  ],
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/styled-components": "^4.1.4"
  }
}
